{
  "name": "handtracking-cursor-control",
  "title": "Handtracking Cursor Control",
  "version": "1.0.0",
  "description": "Control your cursor with you hand",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build"
  },
  "author": "Nico Martin <nico@sayhello.ch>",
  "license": "MIT",
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/chrome": "^0.0.206",
    "dotenv": "^16.0.3",
    "prettier": "^2.8.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "dependencies": {
    "@tensorflow-models/hand-pose-detection": "^2.0.0",
    "@tensorflow/tfjs": "^3.9.0",
    "@tensorflow/tfjs-backend-wasm": "^3.9.0",
    "@tensorflow/tfjs-backend-webgl": "^3.9.0",
    "chrome-tab-identifier": "^0.0.1",
    "preact": "^10.11.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "@babel/plugin-transform-runtime"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "importOrder": [
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
