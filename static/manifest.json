{
  "name": "handtracking-cursor-control-extension",
  "description": "Handtracking Cursor Control Extension",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "assets/contentScript.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "mediapipe/hand_landmark_full.tflite",
        "mediapipe/hand_landmark_lite.tflite",
        "mediapipe/hands.binarypb",
        "mediapipe/hands.js",
        "mediapipe/hands_solution_packed_assets.data",
        "mediapipe/hands_solution_packed_assets_loader.js",
        "mediapipe/hands_solution_simd_wasm_bin.data",
        "mediapipe/hands_solution_simd_wasm_bin.js",
        "mediapipe/hands_solution_simd_wasm_bin.wasm",
        "mediapipe/hands_solution_wasm_bin.js",
        "mediapipe/hands_solution_wasm_bin.wasm"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "scripting"
  ],
  "action": {
    "default_title": "Handtracking Cursor Control Extension",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "48": "/icons/48x.png",
      "128": "/icons/128x.png"
    }
  },
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "48": "/icons/48x.png",
    "128": "/icons/128x.png"
  }
}
